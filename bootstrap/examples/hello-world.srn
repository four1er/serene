(ns examples.hello-world)


(def hello-world
     (fn (name)
         (println "hello" name)))

(def a1
     (fn (x y)
         x))

(defmacro defn
  (name args &body)
  (list 'do
        (list 'def name (cons 'fn (cons args body)))
        (list 'a1 ~args)))





(defn pp (x y)
  (println x))

(def main
     (fn (&args)
         (pp "sam" 2)
         (hello-world "world")))
